<?xml version="1.0"?>
<robot name="rover">
  
  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="Base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="gray">
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="Base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
  </link>

  <!-- Wheels -->
  <link name="front_left_wheel">
    <visual>
      <origin xyz="0.12 0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="front_left_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.12 0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="front_left_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="front_right_wheel">
    <visual>
      <origin xyz="0.12 -0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="front_right_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.12 -0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="front_right_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="back_left_wheel">
    <visual>
      <origin xyz="-0.12 0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="back_left_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.12 0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="back_left_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="back_right_wheel">
    <visual>
      <origin xyz="-0.12 -0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="back_right_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.12 -0.12 -0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="back_right_wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <!-- Lidar -->
  <link name="lidar">
    <visual>
      <origin xyz="0.0 0.0 0.08" rpy="0 0 0"/>
      <geometry>
        <mesh filename="lidar.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link>

  <!-- Camera -->
  <link name="camera">
    <visual>
      <origin xyz="0.15 0.0 0.05" rpy="0 0 0"/>
      <geometry>
        <mesh filename="camera.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link>

  <!-- Battery -->
  <link name="battery">
    <visual>
      <origin xyz="-0.08 0.0 0.02" rpy="0 0 0"/>
      <geometry>
        <mesh filename="battery.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link>

 <!-- Motherboard -->
  <link name="motherboard">
    <visual><origin xyz="0.0 0.0 0.04" rpy="0 0 0"/>
      <geometry><mesh filename="motherboard.stl" scale="0.001 0.001 0.001"/></geometry>
    </visual>
  </link>

  <!-- Joints -->
  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_left_wheel"/>
    <origin xyz="0.12 0.12 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_wheel"/>
    <origin xyz="0.12 -0.12 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="back_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="back_left_wheel"/>
    <origin xyz="-0.12 0.12 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="back_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="back_right_wheel"/>
    <origin xyz="-0.12 -0.12 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="lidar"/>
    <origin xyz="0.0 0.0 0.08" rpy="0 0 0"/>
  </joint>

  <joint name="camera_joint" type="fixed">
    <parent link="base_link"/>
    <child link="camera"/>
    <origin xyz="0.15 0.0 0.05" rpy="0 0 0"/>
  </joint>

  <joint name="battery_joint" type="fixed">
    <parent link="base_link"/>
    <child link="battery"/>
    <origin xyz="-0.08 0.0 0.02" rpy="0 0 0"/>
  </joint>

  <joint name="motherboard_joint" type="fixed">
    <parent link="base_link"/>
    <child link="motherboard"/>
    <origin xyz="0.0 0.0 0.04" rpy="0 0 0"/>
  </joint>

</robot>
